{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "scripts": {
    "dev": "cross-env-shell \"next dev -p ${WEB_PORT:-3001}\"",
    "build": "next build",
    "start": "cross-env-shell \"next start -p ${WEB_PORT:-3001}\"",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "postinstall": "node ./scripts/ensure-app-root.cjs && prisma generate --schema=./prisma/schema.prisma",
    "prebuild": "prisma generate --schema=./prisma/schema.prisma",
    "format": "prettier --check .",
    "test": "playwright test",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@prisma/client": "^6.18.0",
    "@sentry/nextjs": "^10.22.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "google-auth-library": "^10.4.2",
    "next": "14.2.5",
    "next-auth": "^4.24.12",
    "nodemailer": "^7",
    "p-limit": "^7.1.1",
    "pino": "^9.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stripe": "^19.1.0",
    "swr": "^2.3.6",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  }
}
