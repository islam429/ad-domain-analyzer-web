{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env-shell \"next dev -p ${WEB_PORT:-3001}\"",
    "build": "next build",
    "start": "cross-env-shell \"next start -p ${WEB_PORT:-3001}\"",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "test": "playwright test",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cheerio": "^1.1.2",
    "exceljs": "^4.4.0",
    "next": "14.2.5",
    "pino": "^9.4.0",
    "p-limit": "^7.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "prisma": "^5.22.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "playwright": "^1.56.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
